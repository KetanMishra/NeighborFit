{
  "name": "neighborfit-bangalore",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Find your perfect neighborhood in Bangalore based on lifestyle preferences and priorities",
  "author": {
    "name": "Ketan Mishra",
    "email": "ketan.mishra@example.com",
    "url": "https://github.com/KetanMishra"
  },
  "homepage": "https://neighborfitforyou.netlify.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/KetanMishra/NeighborFit.git"
  },
  "bugs": {
    "url": "https://github.com/KetanMishra/NeighborFit/issues"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "analyze": "npx vite-bundle-analyzer dist",
    "clean": "rm -rf dist node_modules/.vite"
  },
  "dependencies": {
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^1.6.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "husky": "^8.0.3",
    "jsdom": "^23.2.0",
    "lint-staged": "^15.5.2",
    "postcss": "^8.4.35",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.19",
    "vite-bundle-analyzer": "^0.7.0",
    "vitest": "^1.6.1"
  },
  "keywords": [
    "neighborhood",
    "bangalore",
    "real-estate",
    "lifestyle",
    "housing",
    "react",
    "typescript",
    "property-finder",
    "location-finder",
    "india",
    "assessment",
    "matching-algorithm",
    "urban-planning",
    "residential"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check && npm run test"
    }
  }
}
